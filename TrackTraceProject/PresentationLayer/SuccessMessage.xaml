<UserControl x:Class="TrackTraceProject.PresentationLayer.SuccessMessage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TrackTraceProject.PresentationLayer"
             mc:Ignorable="d" 
             d:DesignHeight="350" d:DesignWidth="792">
    <Grid>
        <Label x:Name="Lbl_Message" Content="{Binding Message}" HorizontalAlignment="Left" Margin="50,19,0,0" VerticalAlignment="Top"/>

    </Grid>
</UserControl>

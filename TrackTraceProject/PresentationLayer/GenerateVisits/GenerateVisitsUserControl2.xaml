<UserControl x:Class="TrackTraceProject.PresentationLayer.GenerateVisits.GenerateVisitsUserControl2"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TrackTraceProject.PresentationLayer.GenerateVisits"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Label x:Name="Lbl_GeneratedVisits" Content="Generated Visits at {Selected Location} between {Selected StartDate} and {Selected EndDate}" HorizontalAlignment="Left" Margin="50,19,0,0" VerticalAlignment="Top"/>
        <ListBox x:Name="ListBox_GeneratedVisits" HorizontalAlignment="Left" Height="260" Margin="50,50,0,0" VerticalAlignment="Top" Width="692"/>
        <Button x:Name="Btn_CopyToClipboard" Content="Copy To Clipboard" HorizontalAlignment="Left" Margin="630,19,0,0" VerticalAlignment="Top" Width="112" Click="Btn_CopyToClipboard_Click"/>
    </Grid>
</UserControl>
